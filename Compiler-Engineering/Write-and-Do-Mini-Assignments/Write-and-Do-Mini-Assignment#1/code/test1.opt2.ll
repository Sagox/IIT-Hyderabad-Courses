define dso_local i32 @lulz(i32 %n) #0 {
entry:
  %0 = icmp sgt i32 %n, 0
  %smax = select i1 %0, i32 %n, i32 0
  br label %for.cond

for.cond:                                         ; preds = %for.inc, %entry
